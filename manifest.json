{
    "name": "Live Transcript Extension",
    "version": "1.0",
    "description": "Help people to get real time transcript.",
    "manifest_version": 3,
    "key": "",
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "run_at": "document_end",
            "js": [
                "content.js"
            ]
        }
    ],
    "action": {
        "default_title": " transcript",
        "default_icon": "mic.png"
    },
    "oauth2": {
        "client_id": "594020365856-fqpumdvtiu0prd2s2o8i4mp9fp58j9e9.apps.googleusercontent.com",
        "scopes": [
            "https://www.googleapis.com/auth/userinfo.email",
            "https://www.googleapis.com/auth/drive"
        ]
    },
    "permissions": [
        "identity",
        "activeTab",
        "desktopCapture",
        "storage"
    ],
    "content_security_policy": {
"extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'",
 "sandbox": "script-src 'self' 'wasm-unsafe-eval'; sandbox allow-scripts; script-src 'self' 'https://www.googleapis.com'; object-src 'self'"
},


    "background": {
        "service_worker": "background.js"
    }
}
